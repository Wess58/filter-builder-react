{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./dist",
    "emitDeclarationOnly": true,
    "noEmit": false,
    "outDir": "./dist",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "isolatedModules": true,
    "strict": true
  },
  "include": ["src/lib"]
}
